println("Введите координаты Х, а затем У:")
x=tryparse(Float64, readline())
y=tryparse(Float64, readline())
if x>-2 && x<=0
  if y>=0
    if (x+1)^2+y^2<=1 && y<=-(x+1)^2+1
      println("Принадлежит заданной области")
    end
  else 
    if (x+1)^2+y^2<=1
      println("Принадлежит заданной области")
    else
      println("Не принадлежит заданной области")
    end
  end
else
  println("Не принадлежит заданной области")
end
